# .env file
# Credentials for PostgreSQL Admin User
POSTGRES_ADMIN_USER=mole
POSTGRES_ADMIN_PASSWORD=mole_secret
POSTGRES_DB_NAME=moledb

# Credentials for MySQL Root User
MYSQL_ROOT_USER=root # Usually root for MySQL admin
MYSQL_ROOT_PASSWORD=mole_secret
MYSQL_DB_NAME=moledb
MYSQL_REGULAR_USER=mole # Optional: Non-root user for MySQL
MYSQL_REGULAR_PASSWORD=mole_password # Optional: Password for non-root user

# Credentials for InfluxDB Initial Setup & Backend Admin
INFLUXDB_ADMIN_USER=mole
INFLUXDB_ADMIN_PASSWORD=mole_secret
INFLUXDB_ADMIN_ORG=mole-org
INFLUXDB_ADMIN_BUCKET=mole-bucket
# !!! Generate and place your actual InfluxDB All-Access Token here !!!
INFLUXDB_ADMIN_TOKEN=YOUR_ACTUAL_INFLUXDB_ALL_ACCESS_TOKEN

# JWT Secret for Backend
JWT_SECRET=mole-secret-key-change-in-production
